<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane minHeight="500.0" minWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sas.fxml.ApplicationInfoController">
   <children>
      <Label layoutX="50.0" layoutY="25.0" prefHeight="33.0" prefWidth="258.0" text="Application UI Front Page" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="336.0" layoutY="25.0" prefHeight="33.0" prefWidth="258.0" text="Application Info" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <AnchorPane layoutX="10.0" layoutY="70.0" prefHeight="400.0" prefWidth="360.0">
         <children>
            <Label fx:id="lblMsg" layoutX="14.0" layoutY="14.0" prefHeight="353.0" prefWidth="338.0" text="Label" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="376.0" layoutY="70.0" prefHeight="400.0" prefWidth="360.0">
         <children>
            <GridPane layoutX="60.0" layoutY="138.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="15.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="175.0" minWidth="10.0" prefWidth="125.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="206.0" minWidth="10.0" prefWidth="206.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <TextField fx:id="txtUsername" layoutX="60.0" layoutY="138.0" prefWidth="300.0" GridPane.columnSpan="2" />
                  <PasswordField fx:id="passField" prefWidth="300.0" GridPane.columnSpan="2" GridPane.rowIndex="1" />
                  <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#handleCancelButtonAction" prefHeight="40.0" prefWidth="100.0" text="Cancel" GridPane.rowIndex="2" />
                  <Button fx:id="btnLogin" mnemonicParsing="false" onAction="#handleLoginButtonAction" prefHeight="40.0" prefWidth="100.0" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Hyperlink fx:id="hlinkLoginIssue" alignment="CENTER" onAction="#hlinkLoginIssueAction" prefWidth="300.0" text="Sign-in probems due to password or Username?" textAlignment="JUSTIFY" textFill="#2ccfdd" GridPane.columnSpan="3" GridPane.rowIndex="3" />
               </children>
            </GridPane>
         </children>
      </AnchorPane>
   </children>
</Pane>
